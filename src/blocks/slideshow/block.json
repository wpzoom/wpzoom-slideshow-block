{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "wpzoom/slideshow",
	"version": "0.1.0",
	"title": "Slideshow",
	"category": "wpzoom-blocks",
	"icon": "columns",
	"description": "Example block scaffolded with Create Block tool.",
	"supports": {
		"html": false
	},
	"attributes": {
		"uniqueId": {
			"type": "string"
		},
		"autoplay": {
			"type": "boolean",
			"default": true
		},
		"loop": {
			"type": "boolean",
			"default": true
		},
		"speed": {
			"type": "number",
			"default": 1000
		},
		"spaceBetween": {
			"type": "number",
			"default": 100
		},
		"slidesPerView": {
			"type": "number",
			"default": 1
		}
	},
	"textdomain": "wpzoom-slideshow-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}